class Main {

  function void main() {
    var int sum, i;
    var Float mean;
    var Array sample;

    do Stats.init();

    let sample = Array.new(10);
    let i = 0;

    while (i < 10) {
      let sample[i] = i + 1;
      let i = i + 1;
    }

    let sum = Stats.sum(sample, 10);
    let mean = Stats.mean(sample, 10);

    do Output.printInt(sum);
